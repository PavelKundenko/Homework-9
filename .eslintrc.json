{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": ["google"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "no-invalid-this": "off",
        "max-len": ["error", { "code": 100 }],
        "linebreak-style": "off"
    }
}
