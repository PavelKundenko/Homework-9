{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": ["google"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "no-invalid-this": "off",
        //        "semi": ["error", "always"],
//        "comma-dangle": ["error", {
//            "arrays": "always-multiline",
//            "objects": "always-multiline"
//        }],
//        "arrow-parens": ["error", "always"],
        "max-len": ["error", { "code": 100 }]
    }
}
